{
  "name": "template-react-web",
  "version": "1.1.0",
  "description": "react starter",
  "scripts": {
    "dev": "webpack --config ./webpack/dev.js",
    "build": "webpack --config webpack/prod.js",
    "lint": "eslint src/ webpack/",
    "test": "jest --coverage"
  },
  "repository": "git+https://github.com/im6/template-react-web.git",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/template-react-web/issues"
  },
  "homepage": "https://github.com/im6/template-react-web#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react": "^10.0.2",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.3",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "regenerator-runtime": "^0.13.5",
    "uuid": "^7.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "classic"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "prettier",
      "jest"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "prettier/prettier": 2
    },
    "env": {
      "browser": true,
      "jest/globals": true
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "verbose": false,
    "clearMocks": true,
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src/testing"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/testing/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/testing/styleMock.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    }
  },
  "private": false
}
